#include <iostream>
using namespace std;

class Queue {
    int front, rear, size;
    int arr[100];
public:
    Queue() : front(0), rear(-1), size(0) {}
    void enqueue(int val) {
        if (rear == 99) {
            cout << "Queue Overflow\n";
            return;
        }
        arr[++rear] = val;
        size++;
    }
    void dequeue() {
        if (size == 0) {
            cout << "Queue Underflow\n";
            return;
        }
        cout << "Dequeued: " << arr[front++] << "\n";
        size--;
    }
    void display() {
        if (size == 0) {
            cout << "Queue is empty\n";
            return;
        }
        cout << "Queue elements: ";
        for (int i = front; i <= rear; i++) cout << arr[i] << " ";
        cout << "\n";
    }
};

int main() {
    Queue q;
    q.enqueue(10);
    q.enqueue(20);
    q.enqueue(30);
    q.display();
    q.dequeue();
    q.display();
    return 0;
}

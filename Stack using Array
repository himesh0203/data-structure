#include <iostream>
using namespace std;

class Stack {
    int top;
    int arr[100];
public:
    Stack() : top(-1) {}
    void push(int val) {
        if (top == 99) {
            cout << "Stack Overflow\n";
            return;
        }
        arr[++top] = val;
    }
    void pop() {
        if (top == -1) {
            cout << "Stack Underflow\n";
            return;
        }
        cout << "Popped: " << arr[top--] << "\n";
    }
    void display() {
        if (top == -1) {
            cout << "Stack is empty\n";
            return;
        }
        cout << "Stack elements: ";
        for (int i = top; i >= 0; i--) cout << arr[i] << " ";
        cout << "\n";
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.display();
    s.pop();
    s.display();
    return 0;
}

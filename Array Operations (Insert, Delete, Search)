#include <iostream>
using namespace std;

int main() {
    int arr[100] = {1, 2, 3, 4, 5};
    int size = 5;
    
    // Insert element at end
    int val = 6;
    arr[size] = val;
    size++;
    
    // Delete element at index 2
    int deleteIndex = 2;
    for (int i = deleteIndex; i < size - 1; i++) {
        arr[i] = arr[i + 1];
    }
    size--;
    
    // Search for element
    int key = 4;
    int index = -1;
    for (int i = 0; i < size; i++) {
        if (arr[i] == key) {
            index = i;
            break;
        }
    }
    
    cout << "Array elements: ";
    for (int i = 0; i < size; i++) cout << arr[i] << " ";
    cout << "\nElement " << key << (index == -1 ? " not found" : " found at index " + to_string(index)) << endl;
    
    return 0;
}
